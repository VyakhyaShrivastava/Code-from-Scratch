#include<stdio.h>
#include<conio.h>
int n,p,temp;
int f(int n, int p)
{
    int r=1,i;
    for(i=1;i<=p;i++)
    {
        r=r*n;
    }
    return(r);
}
main()
{
    printf("Enter the number ");
    scanf("%d",&n);
    printf("Enter the power to be calculated ");
    scanf("%d",&p);
    temp=f(n,p);
    printf("The result of %d to power %d is %d",n,p,temp);
    getch();
}
