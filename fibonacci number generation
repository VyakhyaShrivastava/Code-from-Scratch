#include<stdio.h>
#include<conio.h>
int n;
int fibo(int n)
{
    if(n==0 || n==1)
        return(n);
        else
            return (fibo(n-1)+fibo(n-2));
}
main()
{
    printf("enter position of the no. whose value you want from the fibonacci series : ");
    scanf("%d",&n);
    int temp = fibo(n);
    printf("%d",temp);
}
