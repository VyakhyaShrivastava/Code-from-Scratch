#include<stdio.h>
#include<conio.h>
main()
{
    int n,i,c=0;
    printf("Enter the number : ");
    scanf("%d",&n);
    if(n>1)
    {
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
            {
             c=1;
             break;
            }
    }
    if (c==0)
    {
        printf("prime");
    }
    else
        printf("not prime");
    }
    else
        printf("not prime");
}
